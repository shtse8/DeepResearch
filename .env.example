# API Keys (required)
OPENAI_API_KEY=your_openai_api_key_here
SERPAPI_KEY=your_serpapi_key_here

# Optional Configuration
# MODEL=gpt4o  # Default model to use (gpt4o, gpt4oMini, etc)
